* {
  box-sizing: border-box; }

body {
  padding: 0;
  margin: 0;
  background-color:#eaeaea;
}

/*Classes*/

/* fixed header*/
.Main-Header-placer{
	margin-top:6px;
	width: 100%;
    z-index: 100;
    position: fixed;
}
.Main-header{
	width: 100%;
}
.Main-header-nav{
	margin:0 20% 0 20%; 
}
.Main-header-nav ul{
    padding: 0px;
    width: 100%;
    margin: auto;
    height: 100%;
	padding-right: 1%;
}
.Main-header-nav li{
	display: inline-block;
    list-style-type: none;
	border-style: solid;
    border-width: 2px;
    border-color: #007fff; 
	border-radius: 10px;
    width: 24%;
    height: 100%;
	margin-left:1%;
    float: left;
    background-color: #fff;
}
.Main-header-nav li a{
    text-align: center;
    text-decoration: none;
    display: block;
    vertical-align: middle;
    width: 100%;
    height: 100%;
    line-height: 175%;
}
.Main-header-nav li a:hover{
background-color: #e0e0e0;
border-radius: 10px;
}
/* header quick contact */
.direct-contact-link-spacer{
	margin-right:5%;
	margin-left:85%;
	width:10%;
	height:50px;

}
.direct-contact-link-spacer a{
	text-align: center;
    text-decoration: none;
    display: block;
    vertical-align: middle;
    width: 100%;
    line-height: 300%;
	background-color: #007fff;
	border-radius: 10px;
}

.direct-contact-link-spacer a:hover{
	background-color: #007fff;
	border-radius: 10px;
}

/* Body home.html */

.Body-Content{
	padding-top:34px;
	margin:0 20% 0 20%; 
}
.Body-Content-Tabviewer{
	margin:1%;
	padding:1%;
	background-color: #fff;
	height:1000px;
	
}

/* tab cycler */
.Body-Content-Tab_Menu{
	margin:4px;
}
.Body-Content-Tab_Menu ul{
    padding: 0px;
    width: 100%;
    margin: auto;

}
.Body-Content-Tab_Menu li{
	display: inline-block;
    list-style-type: none;
}
.Body-Content-Tab_Window{
	background-color: black;
	height:640px;
	margin:4px;
}